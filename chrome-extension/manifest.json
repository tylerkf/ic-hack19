{
  "manifest_version": 2,

  "name": "Cyclops",
  "permissions": [
        "https://www.example.com/",
        "http://146.169.200.26/",
        "activeTab",
        "background"
    ],
  "description": "Using facial recognition to browse through webpages",
  "version": "1.0.0",
  "icons": { "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_icon": "off.png",
    "default_title": "icon"
  },
  "background": {
    "scripts": ["socket.io.slim.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["content.js"]
    }
  ]
}
